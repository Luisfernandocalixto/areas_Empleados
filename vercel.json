{
    "version": 2,
    "routes": [
      {
        "src": "/src/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/src/views/vistas/inicio.hbs"
      }
    ]
  }
  